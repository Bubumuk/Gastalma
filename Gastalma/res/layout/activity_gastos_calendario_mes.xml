<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".GastosCalendarioMesActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal" >

        <TextView
            android:id="@+id/title"
            style="@style/CalendarTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:text="2013" />

        <TableLayout
            android:id="@+id/tableLayout1"
            android:layout_width="match_parent"
            android:layout_height="200dp" >

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1" >

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/enero"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Ene" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/febrero"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Feb." />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/marzo"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Mar" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/abril"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Abr" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="wrap_content"
                android:layout_height="0px"
                android:layout_weight="1" >

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/mayo"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="May" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/junio"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Jun" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/julio"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Jul" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/agosto"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Ago" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow3"
                android:layout_width="wrap_content"
                android:layout_height="0px"
                android:layout_weight="1" >

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/septiembre"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Sep" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/octubre"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Oct" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/noviembre"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Nov" />

                <com.squareup.timessquare.CalendarCellView
                    android:id="@+id/diciembre"
                    style="@style/CalendarCell.CalendarDate"
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Dec" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:layout_alignParentBottom="true" >

        <Button
            android:id="@+id/boton_cal_aceptar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="returnMes"
            android:text="@string/aceptar" />
    </LinearLayout>

</RelativeLayout>